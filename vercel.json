{
    "version": 2,
    "name": "my-app",
    "builds": [
        {
            "src": "web/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "server/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server/dist/src/index.js"
        }
    ]
}